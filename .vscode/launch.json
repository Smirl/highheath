    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Highheath with dev creds",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "env": {
                "TOKEN_FILE": "${workspaceFolder}/token.dev.json"
            },
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/cmd/highheath/main.go",
            "cwd": "${workspaceFolder}"
        }
    ]
}